  git_url("https://#{ENV["GITHUB_USERNAME"]}:#{ENV["GITHUB_PASSWORD"]}@github.com/mathieudutour/fastlane-bruit-blanc")
else
  git_url("https://github.com/mathieudutour/fastlane-bruit-blanc")
end

storage_mode("git")
shallow_clone(true)

type("development") # The default type, can be: appstore, adhoc, enterprise or development

